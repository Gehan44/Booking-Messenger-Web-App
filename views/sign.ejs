<body onselectstart="return false">

  <div id="signature-pad" class="signature-pad">
    <div class="signature-pad--body">
      <canvas></canvas>
    </div>
    <div class="signature-pad--footer">
      <div class="signature-pad--actions">
        <div class="column">
          <button type="button" class="w-30 btn btn-primary btn-lg" data-action="undo" title="Ctrl-Z">ย้อนกลับ</button>
        </div>
        <div class="column">
          <button type="button" class="w-30 btn btn-primary btn-lg" data-action="clear">ลบ</button>
        </div>
        <div class="column">
          <button type="button" class="w-30 btn btn-primary btn-lg" data-action="save">บันทึก</button>
          <input  id="editTerm" class="form-control" type="hidden" value="<%= editTerm %>">
        </div>
        <div class="column">
          <a type="button" class="w-30 btn btn-primary btn-lg" href="/mHome">ออก</a>
        </div>
      </div>
    </div>
  </div>

  <script src="/js/signature_pad.umd.min.js"></script>
  <script src="/js/app.js"></script>
